{
  "version": 2,
  "routes": [
    {
      "src": "/(client-login|client-signup|client-dashboard|quote-form|get-a-quote|login|signup|about|professionals|terms|privacy|forgot-password|reset-password)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "spa": true,
  "fallback": "/index.html"
}